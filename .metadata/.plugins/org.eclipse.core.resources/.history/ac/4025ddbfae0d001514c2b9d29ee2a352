package com.springinaction.zywu;

public class Juggler implements Performer {

	private int bagBeans=3;
	
	public Juggler() {
		// TODO Auto-generated constructor stub
	}
	
	public Juggler(int beanbags){
		this.bagBeans=beanbags;
	}
	
	@Override
	public void perform() {
		System.out.println("Juggler play "+ bagBeans+"beans");
	}

}
