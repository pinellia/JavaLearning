package com.zywu.abstractTest;


abstract class People{
	void eat(){};
	void sleep(){};	
}

class Men extends People
{
	@Override
	void eat(){
		System.out.println("men eat");
	}
}
public class Task1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Men aMen=new Men();
		aMen.eat();
	}

}
